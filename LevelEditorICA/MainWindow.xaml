<Window x:Class="LevelEditorICA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LevelEditorICA"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="20.481" VerticalAlignment="Top" Width="517">
            <MenuItem Header="File" Height="20.481">
                <MenuItem Header="New"/>
                <MenuItem Header="Load"/>
                <MenuItem x:Name="SaveGame" Header="Save"/>
                <MenuItem x:Name="ExitEditor" Header="Exit" Click="ExitEditorCommand"/>
            </MenuItem>
            <MenuItem Header="Options"/>
        </Menu>

    </Grid>
</Window>
